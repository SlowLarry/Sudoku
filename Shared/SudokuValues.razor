@foreach (var text in cellText.Values)
{
    if (text != null)
    {
        <text><text @key="text" x="@text.x" y="@text.y" font-size="@text.fontSize" font-family="@text.fontFamily" dominant-baseline="middle" text-anchor="middle" style="pointer-events: none; fill: @text.color">@text.text</text></text>
    }
}
